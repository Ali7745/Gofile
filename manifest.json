{
  "name": "google-drive-bypass",
  "author": "Manus",
  "title": "Google Drive Direct Download",
  "description": "Bypass Google Drive virus scan warning and get direct download link.",
  "version": "1.0.0",
  "repository": {
    "url": "https://github.com/Manus/gopeed-google-drive-extension"
  },
  "scripts": [
    {
      "event": "onResolve",
      "match": {
        "urls": [
          "*://drive.google.com/file/d/*",
          "*://drive.google.com/uc*",
          "*://drive.usercontent.google.com/download*"
        ]
      },
      "entry": "index.js"
    }
  ]
}
